          "fontSize": 10,
          "fontWeight": "bold"
        },
        "encoding": {
          "text": {"value": ["17%"]}, 
          "color": {"value": "white"},   
          "x": {"value": 200},               
          "y": {"value": 150}              
        }
      }
    ],
    "background": "#f2e2e000",
    "config": {
      "legend": {
        "orient": "left",
        "labelColor": "white",
        "labelFontSize": 10,
        "titleOpacity": 0
      },
      "style": {
        "cell": {
          "stroke": "transparent"
        }
      }
    }
  }
  